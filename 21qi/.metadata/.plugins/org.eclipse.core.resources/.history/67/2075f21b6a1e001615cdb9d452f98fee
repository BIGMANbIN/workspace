package kaishengit;

public class Test {
	// 1.¶ñººÊ½
	private Test(){
		
	}
	private static Test test = new Test();
	
	public static Test getInstance(){
		return test;
	}
	
//	7.µİ¹éËã·¨
	public static void main(String[] args) {
		System.out.println(a(8));
	}
	static int a(int n){
		if(n == 1){
			return 10;
		}
		return (n-1) * 2 + 10;
		
	}
}
	
	



